text_1 = input()
text_2 = input()
lenght_1 = len(text_1)  ##ohne 
lenght_2 = len(text_2)  ##ohne
result_1 = ""
result_2 = ""
 
for i in range (0, lenght_1): #len(text_1_
    result_2 += text_2[i]
    result_1 = ""      #SEHR WICHTIG 
    for n in range (0,lenght_2):   # len(text_2)
        if n <= i:
            continue
        result_1 += text_1[n]
 
    if not text_1[i] == text_2[i]:
        print(result_2 + result_1)
